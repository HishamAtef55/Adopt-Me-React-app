{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:jsx-a11y/recommended",
        "plugin:react-hooks/recommended"
        // "plugin:import/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react","jsx-a11y","import"
    ],
    "rules": {
        "react/prop-types":0,
        "react/react-in-jsx-scope":0
    },
    "settings": {
        "react":{
            "version":"detect"
        },
        "import/resolver":{
            "node":{
                "extensions":["js","jsx"]
            }
        }
    }
}
